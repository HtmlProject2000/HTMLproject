package classesModelo;

import java.util.Date;

public class evento {
	String nome, descricao, categoria, elaborador, local;
	double pontuacao;
	Date dataOcorrencia;
	int qndPrevistaPessoas;
	public evento(String nome, String descricao, String categoria, String elaborador, String local, double pontuacao, Date dataOcorrencia, int qndPrevistaPessoas){
		this.descricao = descricao;
		this.categoria = categoria;
		this.elaborador = elaborador;
		this.local = local;
		this.pontuacao = pontuacao;
		this.dataOcorrencia = dataOcorrencia;
		this.qndPrevistaPessoas= qndPrevistaPessoas;
	}
}
